{
  "addDevDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.1"
  },
  "instructions": [
    "Run: pnpm -w add -D -w @typescript-eslint/eslint-plugin@^6.0.0 @typescript-eslint/parser@^6.0.0",
