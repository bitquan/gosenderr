{
  "firestore": {
    "rules": "firebase/firestore.rules"
  },
  "storage": {
    "rules": "firebase/storage.rules"
  },
  "functions": {
    "source": "firebase/functions"
  },
  "hosting": {
    "site": "gosenderr-6773f",
    "source": "apps/web",
    "ignore": ["firebase.json", "**/node_modules/**"],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ],
    "frameworksBackend": {
      "region": "us-central1",
      "runtime": "nodejs20",
      "omit": [],
      "maxInstances": 10,
      "minInstances": 0,
      "concurrency": 80
    }
  }
}
