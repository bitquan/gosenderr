{
  "name": "@gosenderr/marketplace-app",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx",
    "test:e2e": "bash ../../scripts/start-emulators.sh && playwright test tests/e2e --config=playwright.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "test": "vitest",
    "cap:sync": "pnpm build && npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:run:ios": "pnpm cap:sync && npx cap run ios",
    "cap:run:android": "pnpm cap:sync && npx cap run android"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/camera": "^8.0.0",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/geolocation": "^8.0.0",
    "@capacitor/ios": "^8.0.1",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@gosenderr/shared": "workspace:*",
    "@gosenderr/ui": "workspace:*",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.3",
    "browser-image-compression": "^2.0.2",
    "clsx": "^2.1.1",
    "firebase": "^12.0.0",
    "framer-motion": "^12.27.5",
    "mapbox-gl": "^3.9.1",
    "ngeohash": "^0.6.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.3",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/mapbox-gl": "^3.4.0",
    "@types/ngeohash": "^0.6.8",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react-swc": "^4.0.2",
    "autoprefixer": "^10.4.23",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vitest": "^0.34.7",
    "@testing-library/react": "^14.0.0"
  }
}
